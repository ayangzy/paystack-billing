{
    "name": "ayangzy/cashier-paystack",
    "description": "A Laravel Cashier Wrapper that provides an expressive, fluent interface to Paystack's subscription invoicing services.",
    "keywords": ["laravel", "paystack", "billing", "cashier"],
    "license": "MIT",
    "authors": [
        {
            "name": "Felix Ayange",
            "email": "ayangzy@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3|^8.2",
        "dompdf/dompdf": "^2.0.3",
        "illuminate/database": "^10.22.0",
        "illuminate/support": "^10.22.0",
        "nesbot/carbon": "^2.67",
        "symfony/http-kernel": "^6.2",
        "unicodeveloper/laravel-paystack": "^1.1.0"
    },
    "require-dev": {
        "illuminate/http": "^10.22.0",
        "illuminate/routing": "^10.22.0",
        "illuminate/view": "^10.22.0",
        "mockery/mockery": "^1.6.6",
        "orchestra/testbench": "^8.10.1",
        "phpunit/phpunit": "^10.3.3"
    },
    "autoload": {
        "psr-4": {
            "ayangzy\\Cashier\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ayangzy\\Cashier\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ayangzy\\Cashier\\CashierServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
